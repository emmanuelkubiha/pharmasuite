        <script type="text/javascript">

        $('.btn_voir').on('click',function(e){
          e.preventDefault();

          const id_produit = $(this).attr('id_produit');
          const titre_produit = $(this).attr('titre_produit');

          document.getElementById('titre-produit').innerHTML = titre_produit;
