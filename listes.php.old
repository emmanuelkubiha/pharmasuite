          var demoParams = [
            
            {
              title: titre_produit,
              config: {
                text: id_produit, // Content

                width: 240, // Widht
                height: 240, // Height
                colorDark: "#000000", // Dark color
                colorLight: "#ffffff", // Light color
                            
                //quietZone: 15,
                //quietZoneColor: '#307fdd',

                // === Logo
